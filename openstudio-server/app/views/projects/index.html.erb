<h1>Listing projects</h1>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Action</th>
  </tr>
<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, project_path(project) %></td>
    <td><%= link_to 'Edit', edit_project_path(project) %> | <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
