<h1>Snow Log</h1>

<h2>Logs</h2>
<script>
    $(document).ready(function () {
            $("#datapointtable").tablesorter();
        }
    );

</script>

<table id="datapointtable" class="tablesorter table table-striped">
  <thead>
  <tr>
    <th>SNOW Cluster Messages</th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><%= @snow_log.gsub("\n", "</br>").html_safe unless @snow_log.nil? %></td>
  </tr>
  </tbody>
</table>
